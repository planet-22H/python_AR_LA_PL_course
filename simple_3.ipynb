        "# 3.3 第三份 西安城市数及区域数统计\n",
        "data = pandas.read_json('./data/poi_8_education.json')\n",
        "d = data.groupby([\"city\",\"area\"])\n",
        "result = {}\n",
        "for k in d.groups.keys():\n",
        "    if k[0] not in result:\n",
        "        result[k[0]] = []\n",
        "    result[k[0]].append(k[1])\n",
        "result"
      ]
    }
